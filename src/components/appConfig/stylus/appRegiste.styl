
.appRegiste
  background: #fff
  padding-top: 0!important
  .container
    width: 300px
    margin: 20px auto
  .headerFace
    border-radius: 100% 100%
    display: block
    margin: 0 auto 10px
  input
    color:#666
    font-size: 15px
  .ivu-icon
    color: #8686BC
  .ivu-icon-ios-person
    font-size: 20px
    width: 16px
  .ivu-form-inline .ivu-form-item
    width: 300px
  .ivu-form-item-error-tip
    padding: 5px 10px 0
    font-size: 12px
  .ivu-icon-ios-person:before
    margin-left: -2px
  .onLogin
    text-align: center
    color: #999
    position: fixed
    width: 100%
    bottom: 10px
    left: 0
    span
      font-size: 12px
    span:nth-child(2)
      color: #0000cc
  .phone
    width: 100%
  .phoneCode
    width: 25%
    padding: 9px 2px
    overflow: hidden
    float: right
    margin-top: -35px
    span
      font-size: 12px